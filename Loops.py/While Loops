#1

my_str = "hello"
i = 0 # counter variable
while i < 5:
  char = my_str[i]
  print(char)
  i += 1
 
#2

numbers = [7, 7, 2, 7, 11]
i = 0
while i < 5:
  char = numbers[i]
  print(char)
  i +=1

#3

marks = [84, 89, 90, 45, 67]
i = 0
while i < 5:
  char = marks[i]
  print(char)
  i +=1

#4

for i in range(10):
  print(i)

#5

for i in range (5, 100, 5):
  print(i)

#6 

for i in range (100, -1, -5):
  print(i)

